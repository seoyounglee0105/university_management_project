<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.green.university.repository.interfaces.CollegeRepository">
	<insert id="insert">
		insert into college_tb (name)
		values(#{name})
	</insert>
	<select id="findAll" resultType="com.green.university.repository.model.College">
		select * from college_tb order by id
	</select>
	<delete id="delete">
		delete from college_tb where id = #{id}
	</delete>
</mapper>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.green.university.repository.interfaces.DepartmentRepository">
	<insert id="insert">
		insert into department_tb (name, college_id)
		values(#{name}, #{collegeId})
	</insert>
	<select id="findAll" resultType="com.green.university.repository.model.Department">
		select * from department_tb
	</select>
	<delete id="delete">
		delete from department_tb where college_id = #{collegeId}
	</delete>
</mapper>
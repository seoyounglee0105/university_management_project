<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.green.university.repository.interfaces.CollTuitRepository">
	<insert id="insert">
		insert into coll_tuit_tb (college_id, amount)
		values(#{collegeId}, #{amount})
	</insert>
	<select id="findAll" resultType="com.green.university.repository.model.CollTuit">
		select * from collTuit_tb
	</select>
</mapper>